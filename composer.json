{
    "name": "dainsys/timy2",
    "description": "Add user's time tracker functionality to Laravel 7, Livewire and Bootstrap 4",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.1 || ^5.7 || ^5.8 || ^6.0 || ^7.0",
        "dainsys/components": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/ui": "^2.0",
        "livewire/livewire": "dev-master",
        "maatwebsite/excel": "^3.1",
        "pusher/pusher-php-server": "~4.0"
    },
    "require-dev": {
        "orchestra/testbench": "5.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yismen.jorge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dainsys\\Timy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dainsys\\Timy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\Timy\\TimyServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-package-update": [
            "@php artisan vendor:publish --tag=timy-components --force"
        ]
    }
}
