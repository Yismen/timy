{
    "name": "dainsys/timy",
    "description": "Add user's time tracker functionality to Laravel 7, VueJs 2 and Bootstrap 4",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "^7.0",
        "dainsys/components": "^1.0",
        "laravel/ui": "^2.0",
        "maatwebsite/excel": "^3.1",
        "pusher/pusher-php-server": "~4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yismen.jorge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dainsys\\Timy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dainsys\\Timy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\Timy\\TimyServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-package-update": [
            "@php artisan vendor:publish --tag=timy-components --force"
        ]
    }
}