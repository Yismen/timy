{
    "name": "dainsys/timy2",
    "description": "Add user's time tracker functionality to Laravel 7, Livewire and Bootstrap 4",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "dainsys/components": "^1.0",
        "consoletvs/charts": "6.*",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "laravel/ui": "^3.0",
        "livewire/livewire": "^v2.0",
        "maatwebsite/excel": "^3.1",
        "pusher/pusher-php-server": "~4.0"
    },
    "require-dev": {
        "orchestra/testbench": "6.x-dev",
        "laravel/legacy-factories": "1.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yismen.jorge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dainsys\\Timy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dainsys\\Timy\\Tests\\": "tests/",
            "App\\": "tests/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\Timy\\TimyServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-package-update": [
        ]
    }
}
